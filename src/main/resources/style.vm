    /*---------------------
    Basic Groups
    ---------------------*/

    #header {
        background-color:rgba(0,0,0,0.5);
        color:white;
        text-align:center;
        font-family:Helvetica,Arial,sans-serif;
        padding:5px;
    }
    #content {
        width:30em;
        margin-left:auto;
        margin-right:auto;
        float:left;
        padding:10px;
    }
    #footer {
        background-color:rgba(0,0,0,0.6);
        color:white;
        clear:both;
        text-align:center;
        font-family:Helvetica,Arial,sans-serif;
