    /*--------------------
    Document Customization
    --------------------*/
    html {
        background:#bbbbbb;
        background-image:url(${channel.bgImage});
        background-repeat:no-repeat;
        background-attachment:fixed;
        background-position:center;
    }
    /*-------------------
    Main Table w/ Customization
    -------------------*/
    table {
        color:rgba(255,255,255,0.5);
        font-family:Helvetica,Arial,sans-serif;
        width:640px;
        border-collapse:collapse;
        border-spacing:0;
        align-self: baseline;
    }
    tr, td {
        border:1px solid transparent;
        height:30px;
        transition:all 0.3s;
        color: black;
    }
    th {
        background:#0f6d84;
        font-weight:bold;
        color: darkblue;
    }
    td {
        background:rgba(200,229,242,0.7);
        text-align:center;
    }
    tr:nth-child(even) td {
        background:rgba(124,203,223,0.7);
    }
    tr:nth-child(odd) td {
        background:rgba(145,215,232,0.6);
    }
    tr td:hover {
        background:rgba(43,201,240,0.8);
        color:black;
    }
    /*---------------------
    General Text Customization
    ---------------------*/
    a:link {
        color:rgb(0,0,0);
    }
    a:visited {
        color:purple;
    }
    /*-------------------
    Sidepanel Nav Buttons
    -------------------*/
    #nav {
        float:left;
        padding:5px;
    }
    #nav a:link {
        color:#23BADF;
    }
    .commands {
        transition-duration:0.4s;
        /*background-color:#0f6d84;*/
        border:1px solid #23BADF;
        color:white;
        padding:15px 32px;
        text-align:center;
        text-decoration:none;
        display:inline-block;
        font-size:16px;
        font-family:Helvetica,Arial,sans-serif;
        margin-bottom:10px;
    }
    .commands:hover {
        background-color:#0f6d84;
        color:white;
    }
