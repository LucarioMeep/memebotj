<html>
<head>
    <meta charset="UTF-8">
    <title>Commands for ${channel.channel}</title>
</head>

<body>
#parse("include.vm")
<div id="content">
    <table style="width:100%" class="users">
        <tr>
            <td>Name</td>
        </tr>
        #foreach($name in $userlist)
            <tr>
                <td><a href="${web}/users/listnames/${channel.broadcaster}/${name}">${name}</a></td>
            </tr>
        #end
    </table>
</div>
#parse("footer.vm")
</body>

</html>